{
  "compilerOptions": {
    "jsx": "react",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/stores/*": [
        "./src/stores/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ],
      "@/modelBase/*":["./src/modelBase/*"]
    }
  }
}
